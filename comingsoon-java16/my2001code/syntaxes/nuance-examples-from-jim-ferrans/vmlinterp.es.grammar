;;
;; key.grammar-v6
;;
;; Grammars for touch-tone key sequences
;;
;;  The standard "words" for touch-tone digits 0-9 are "dtmf-0" ... "dtmf-9",
;;   respectively.  The '*' (asterisk/star) key is denoted by "dtmf-star",
;;   and the '#' (pound) key is denoted by the word "dtmf-pound".
;;
;;  This file defines the following grammars:
;;
;;      KeyDigit
;;                 - Returns a single digit corresponding to a single touchtone
;;                   key press
;;
;;      Key1Digit ...Key12Digits
;;                 - Returns an integer corresponding to the specified number
;;                   of touchtone key presses.  Note that leading zeroes will
;;                   be omitted.
;;
;;      Key1DigitString...Key12DigitString
;;                 - Returns a string corresponding to the specified number of
;;                   touchtone key presses.  Note that leading zeroes will not
;;                   be omitted.

KeyDigit   [
     dtmf-0         {return(0)}
     dtmf-1         {return(1)}
     dtmf-2         {return(2)}
     dtmf-3         {return(3)}
     dtmf-4         {return(4)}
     dtmf-5         {return(5)}
     dtmf-6         {return(6)}
     dtmf-7         {return(7)}
     dtmf-8         {return(8)}
     dtmf-9         {return(9)}
]

Key1Digit (
     KeyDigit:d
)                   {return($d)}

Key2Digits (
     KeyDigit:d1
     KeyDigit:d2
)                   {return(add(times($d1 10) $d2))}

Key3Digits (
     Key2Digits:n
     KeyDigit:d
)                   {return(add(times($n 10) $d))}

Key4Digits (
     Key3Digits:n
     KeyDigit:d
)                   {return(add(times($n 10) $d))}

Key5Digits (
     Key4Digits:n
     KeyDigit:d
)                   {return(add(times($n 10) $d))}

Key6Digits (
     Key5Digits:n
     KeyDigit:d
)                   {return(add(times($n 10) $d))}

Key7Digits (
     Key6Digits:n
     KeyDigit:d
)                   {return(add(times($n 10) $d))}

Key8Digits (
     Key7Digits:n
     KeyDigit:d
)                   {return(add(times($n 10) $d))}

Key9Digits (
     Key8Digits:n
     KeyDigit:d
)                   {return(add(times($n 10) $d))}

Key10Digits (
     Key9Digits:n
     KeyDigit:d
)                   {return(add(times($n 10) $d))}

Key11Digits (
     Key10Digits:n
     KeyDigit:d
)                   {return(add(times($n 10) $d))}

Key12Digits (
     Key11Digits:n
     KeyDigit:d
)                   {return(add(times($n 10) $d))}

Key1DigitString [
     dtmf-0         {return("0")}
     dtmf-1         {return("1")}
     dtmf-2         {return("2")}
     dtmf-3         {return("3")}
     dtmf-4         {return("4")}
     dtmf-5         {return("5")}
     dtmf-6         {return("6")}
     dtmf-7         {return("7")}
     dtmf-8         {return("8")}
     dtmf-9         {return("9")}
]

Key2DigitString   (
     Key1DigitString:d1
     Key1DigitString:d2
)                          {return(strcat($d1 $d2))}

Key3DigitString   (
     Key1DigitString:d1
     Key2DigitString:d2
)                          {return(strcat($d1 $d2))}

Key4DigitString   (
     Key1DigitString:d1
     Key3DigitString:d2
)                          {return(strcat($d1 $d2))}

Key5DigitString   (
     Key1DigitString:d1
     Key4DigitString:d2
)                          {return(strcat($d1 $d2))}

Key6DigitString   (
     Key1DigitString:d1
     Key5DigitString:d2
)                          {return(strcat($d1 $d2))}

Key7DigitString   (
     Key1DigitString:d1
     Key6DigitString:d2
)                          {return(strcat($d1 $d2))}

Key8DigitString   (
     Key1DigitString:d1
     Key7DigitString:d2
)                          {return(strcat($d1 $d2))}

Key9DigitString   (
     Key1DigitString:d1
     Key8DigitString:d2
)                          {return(strcat($d1 $d2))}

Key10DigitString   (
     Key1DigitString:d1
     Key9DigitString:d2
)                          {return(strcat($d1 $d2))}

Key11DigitString   (
     Key1DigitString:d1
     Key10DigitString:d2
)                          {return(strcat($d1 $d2))}

Key12DigitString   (
     Key1DigitString:d1
     Key11DigitString:d2
)                          {return(strcat($d1 $d2))}

