           (myDigit:d1 myDigit:d2 myDigit:d3 myDigit:d4 myDigit:d5 myDigit:d6 ?dtmf-pound)
             {<vmlslot0 strcat($d1 strcat($d2 strcat($d3 strcat($d4 strcat($d5 $d6)))))>}
          ]
         ?Thanks
)
 
.Pin ( ?VUh
       ?(?my ?(four digit) pin ?number ?is)
       [
