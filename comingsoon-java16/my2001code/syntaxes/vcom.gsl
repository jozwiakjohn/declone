]) {return($s)}

STK_MOT     ([motorola (moto rola)] ?_SEMICONDUCTOR ?_INC)    {return("*stkMOT")}
STK_INTC     ([([(in tell) intel] ?[_SEMICONDUCTOR computer] ?_CORP) i_n_t_c (i n t c)])    {return("*stkINTC")}
STK_MSFT     ([([microsoft (micro soft)] ?_CORP) m_s_f_t (m s f t)])    {return("*stkMSFT")}
STK_DELL     (dell ?[computer computers] ?_CORP)    {return("*stkDELL")}
STK_IBM     ([(international business [machine machines]) (i b m) i_b_m (big blue)] ?_CORP)    {return("*stkIBM")}

_SEMICONDUCTOR	[semiconductor semiconductors (semi ?[conductor conductors])] 
