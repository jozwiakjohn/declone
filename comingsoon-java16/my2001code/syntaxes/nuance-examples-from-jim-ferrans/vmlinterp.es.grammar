YEAR [

	;; ninety six
	;; nineteen ninety six

	(
		
		(?(mil novecientos)
		[ 	([(noventa y seis) noventa_y_seis])     	{return (1996)}
           		([(noventa y siete) noventa_y_siete])   	{return (1997)}
           		([(noventa y ocho) noventa_y_ocho])  	  	{return (1998)}
        		([(noventa y nueve) noventa_y_nueve])     	{return (1999)}
          
		]
		)
		
      	)

	;; two thousand
	;; the year two thousand

	(?(el ano) dos mil dos_mil)    {return (2000)}


	;; two thousand five
	;; two thousand and five
	;; twenty oh five
	
	((dos mil)
		[	uno       {return (2001)}
			dos       {return (2002)}
			tres     {return (2003)}
			cuatro      {return (2004)}
			cinco      {return (2005)}
			seis       {return (2006)}
			siete     {return (2007)}
			ocho     {return (2008)}
			nueve      {return (2009)}
		]
	)

]

;; added separate catagory for now
